{
  "name": "@openclaw/dojo-genesis-plugin",
  "version": "1.0.0",
  "description": "Specification-driven development orchestration for OpenClaw",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/",
    "skills/",
    "hooks/",
    "openclaw.plugin.json",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "openclaw": {
    "extensions": "index.ts"
